<!DOCTYPE form-validation PUBLIC
        "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.3.0//EN"
        "http://jakarta.apache.org/commons/dtds/validator_1_3_0.dtd">

<form-validation>
  <formset>
  	<form name="LoginForm">
  		<field property="username" depends="required">
  			<arg key="error.username.required"/>
  			<msg name="required" key="error.username.required" />
  		</field>
  		<field property="password" depends="required">	
  			<arg key="error.password.required"/>
  			<msg name="required" key="error.password.required" />
  		</field>
  		<field property="role" depends="required">
  			<arg key="error.role.required"/>
  			<msg name="required" key="error.role.required" />
  			<!--  <var>  
                <var-name>test</var-name>  
             	<var-value>(*this* == "NULL")</var-value>  
             </var> -->
  		</field>
  	</form>
  </formset>

