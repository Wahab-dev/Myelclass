	<queryString language="SQL">
		<![CDATA[SELECT 
	form.sampleno, DATE_FORMAT(orderdt, '%e/%c/%y') as orderdt,  del.shortform as Deliver, tan.tanshform as Tannery,  cust.shortform as Customer, 
	concat (articletype,' ' ,articlename) as Article,  color, size,  LEFT(substance, LENGTH(substance) - 2) as substance, selection, quantity, 
case 
		 when (unit = 'Skin') then ''
		 when (unit = 'Skins') then ''
		 when (unit = 'sqft') then 'sqft'
		 else unit
	 end as unit, 
	rate,  shpd, bal,  DATE_FORMAT(cdd_date, '%e/%c/%y') as cdd_date, DATE_FORMAT(add_date, '%e/%c/%y') as add_date, DATE_FORMAT(rdd_date, '%e/%c/%y') as rdd_date, courierdetails 
FROM  
	elpro.tbl_srf_article article 
left outer join
	elpro.tbl_srfarticle_status stats on article.srfarticleid = stats.srfarticleid 
left outer join 
	elpro.tbl_srfform form on form.sampleno = article.sampleno 
left outer join 
	elpro.tbl_customer cust on cust.custid = form.customerid 
left outer join 
	elpro.tbl_tannery tan on tan.tanid = form.tanneryid 
left outer join 
	elpro.tbl_customer del on del.custid = form.customerid 
where 
	status in ('P','IC','RW')
order by 
	Tannery asc, form.sampleno asc, article.articletype asc, article.articlename asc, article.color asc]]>
	</queryString>
